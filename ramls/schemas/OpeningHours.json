{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Opening Hours",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "calendarId": {
      "type": "string"
    },
    "descriptionId": {
      "type": "string"
    },
    "eventType": {
      "type": "string"
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "format": "date-time"
    },
    "allDay": {
      "type": "boolean"
    },
    "open": {
      "type": "boolean"
    },
    "active": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "calendarId",
    "descriptionId",
    "eventType",
    "startDate",
    "endDate"
  ]
}
