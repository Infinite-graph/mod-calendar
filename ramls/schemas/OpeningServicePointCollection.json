{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Opening collection",
  "type": "object",
  "properties": {
    "servicePoints": {
      "servicePointId": {
        "type": "string",
        "example": "10"
      },
      "openingPeriods": {
        "type": "array",
        "id": "Openings",
        "items": {
          "type": "object",
          "$ref": "OpeningHours.json"
        }
      }
    },
    "totalRecords": {
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "required": [
    "openings",
    "totalRecords"
  ]
}
