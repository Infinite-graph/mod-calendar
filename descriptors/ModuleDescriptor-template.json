{
  "id": "${artifactId}-${version}",
  "name": "Calendar module",
  "provides": [
    {
      "id": "calendar",
      "version": "1.0",
      "handlers": [
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/calendar/events"
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/calendar/edit/events",
          "permissionsRequired": [
            "calendar.collection.get"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/calendar/edit/addevents",
          "permissionsRequired": [
            "calendar.collection.add"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/calendar/edit/addexceptions",
          "permissionsRequired": [
            "calendar.collection.add"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/calendar/edit/listexceptions",
          "permissionsRequired": [
            "calendar.collection.get"
          ]
        }
      ]
    }
  ],
  "permissionSets": [
    {
      "permissionName": "calendar.collection.get",
      "displayName": "List calendar event descriptions",
      "description": ""
    },
    {
      "permissionName": "calendar.collection.add",
      "displayName": "Add new calendar event description",
      "description": ""
    },
    {
      "permissionName": "calendar.collection.all",
      "displayName": "Calendar permissions",
      "description": "",
      "subPermissions": [
        "calendar.collection.get",
        "calendar.collection.add"
      ],
      "visible": true
    }
  ]
}
